<h1>Listing story_texts</h1>

<table>
  <tr>
  </tr>

<% for story_text in @story_texts %>
  <tr>
    <td><%= link_to 'Show', story_text %></td>
    <td><%= link_to 'Edit', edit_story_text_path(story_text) %></td>
    <td><%= link_to 'Destroy', story_text, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New story_text', new_story_text_path %>
